
@{
    ViewData["Title"] = "Hello World 2";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title> @ViewData["Title"]</title>
    <link href="~/css/site.css" rel="stylesheet" />

</head>
<body>
 
    <header>
    
        <h1>Welcome to My Page</h1>
        <menu>
            <ul>
                <li><a asp-controller="App"  asp-action="Index"  >Home</a></li>
                <li><a asp-controller="App"  asp-action="About" >About</a></li>
                <li><a asp-controller="App"  asp-action="Contact" >Contact</a></li>


            </ul>





        </menu>
    
    </header>
    <section>
        <h3>@ViewData["Title"]</h3>
        @RenderBody()
    </section>
    <footer>Copyright 2018 MY LLC</footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/index.js"></script>
</body>
</html>